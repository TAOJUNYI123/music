/* pages/login/register/Register.wxss */
.page{
    width: 100%;
    font-size: 14px;
    overflow: hidden;
    display: flex;
    align-items: center;
    justify-content: center;
}
.backgroundImg{
    width: 100%;
    height: 100%;
    position: absolute;
    z-index: -1;
}
.registerBox{
    width: 80%;
    height: 300px;
}
.name,.pwd,.surePwd{
    width: 100%;
    height: 44px;
    display: flex;
    align-items: center;
    justify-content: space-between;
    border-bottom: 1px solid #199ABA;
    margin-bottom: 15px;
}
.name image,.pwd image,.surePwd image{
    width: 22px;
    height: 22px;
}
.name input,.pwd input,.surePwd input{
    width: 80%;
    margin-right: 20px;
}
.registerBtn{
    width: 100%;
    margin-top: 25px;
}