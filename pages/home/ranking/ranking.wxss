/* pages/home/ranking/ranking.wxss */
.titleBox{
    width: 100%;
    height: 44px;
    display: flex;
    align-items: center;
}
.titleBox .left{
    width: 16px;
    height: 10px;
    background-color: #199ABA;
}
.titleBox .right{
    font-size: 15px;
    font-weight: bold;
    margin-left: 10px;
}
.contentBox{
    width: 92%;
    height: 120px;
    margin: 0 auto;
    border-radius: 20px;
    background-color: #EDECE8;
}
.contentBox:last-child{
    margin-bottom: 15px;
}
.ranking{
    color: #D62C25;
    font-weight: bold;
}
.item{
    padding-top: 8px;
    margin-left: 5px;
    overflow:hidden;
    text-overflow:ellipsis;
    white-space:nowrap;
}